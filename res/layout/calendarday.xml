<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:id="@+id/DayLayout"
	android:paddingBottom="10px" android:background="@color/calendar_bg"
	android:keepScreenOn="true">

	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/DayDateText"
		android:text="1" android:layout_gravity="top|right" android:textColor="@color/calendar_text"
		android:textColorHighlight="@color/calendar_text_highlight"
		android:textSize="10px"></TextView>
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="@color/calendar_data_text"
		android:textColorHighlight="@color/calendar_data_text"
		android:gravity="center" android:layout_gravity="center" android:id="@+id/DayDataText"
		android:text=" " android:paddingTop="10px"></TextView>
</FrameLayout>
